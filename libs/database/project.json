{
  "name": "database",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/database/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:shared"],
  "targets": {
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd libs/database/prisma && npx prisma generate"
      }
    },
    "migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd libs/database/prisma && npx prisma migrate dev"
      }
    },
    "migrate:deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd libs/database/prisma && npx prisma migrate deploy"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd libs/database/prisma && npx prisma studio"
      }
    }
  }
}
